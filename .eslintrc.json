{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "google"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
        }
    },
    "rules": {
        "require-jsdoc": 0,
        "indent": ["error", 4],
        "max-len": ["error", 120]
    }
}
